CC=g++ -std=c++11
LN=g++
ALL: Prob1
Prob1:
	@echo "Prob1"
	@echo "compiling and linking the code"
	$(CC) -c p1.cpp
	$(LN) -o p1 p1.o
	@echo "running, usage: ./p1 sample1"
	./p1 raw/sample1.raw

clean:
	rm -f *.o *.raw *.data p1